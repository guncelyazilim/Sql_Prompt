    <Snippet>
      <Declarations>
        <Literal>
          <ID>TabloAdi</ID>
          <ToolTip />
          <Default />
        </Literal>
      </Declarations>
      <Code Language="sql"><![CDATA[CREATE Trigger dbo.$TabloAdi$_Update  ON [dbo].[$TabloAdi$]
FOR Update
AS
	/* Yazan  : $USER$
		Tarih : $DATE$
	*/
SET NOCOUNT ON
BEGIN
	BEGIN TRY
	Declare 
	/* Cursor Variables*/
	Declare c$TabloAdi$_Update Cursor For
	Select 
	From	inserted i 
	INNER JOIN deleted d On i. = d.
	Open c$TabloAdi$_Update
	Fetch Next From c$TabloAdi$_Update Into
	While @@Fetch_Status=0 Begin

		Fetch Next From c$TabloAdi$_Update Into
	End
	Close c$TabloAdi$_Update
	Deallocate c$TabloAdi$_Update
	END TRY
	BEGIN CATCH
		Declare @ITObjectName nvarchar(126)
		Set @ITObjectName = OBJECT_NAME (@@PROCID)
		Execute SP15_GenerateError @ITObjectName
	END CATCH
END
]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>