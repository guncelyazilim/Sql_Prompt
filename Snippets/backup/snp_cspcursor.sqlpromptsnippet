    <Header>
      <Title>snp_cspcursor</Title>
      <Shortcut>snp_cspcursor</Shortcut>
      <Description>Create SP CURSOR</Description>
      <Author />
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>PSP_xx</ID>
          <ToolTip />
          <Default />
        </Literal>
      </Declarations>
      <Code Language="sql"><![CDATA[Create Procedure  [dbo].[$PSP_xx$] 
As
Set NoCount On
Begin 
	/*
		Yazan	: $USER$
		Tarih	: $DATE$
		Açıklama:
	*/
    Begin Try
		Declare
		/*Cursor variables*/
		Declare c$PSP_xx$ Cursor Local For
		Select
		From
		Where
		Open c$PSP_xx$
		Fetch Next From c$PSP_xx$ Into
		While @@FETCH_STATUS=0 Begin
			
			Fetch Next From c$PSP_xx$ Into
		End
		Close c$PSP_xx$
		Deallocate c$PSP_xx$
        
    End Try 
    Begin Catch	
        Declare @ITObjectName NVarchar(126);
        Set @ITObjectName = OBJECT_NAME(@@PROCID);
        Execute SP15_GenerateError @ITObjectName;
    End Catch
End
Go


]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>